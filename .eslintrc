{
  "extends": "eslint-config-airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "prefer-const": 0,
    "comma-dangle": 0,
    "radix": 0,
    "no-irregular-whitespace": 0,
    "no-else-return": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "default-case": 0,
    "no-nested-ternary": 0,
    "one-var": 0
  },
  "plugins": [
    "react"
  ],
  "globals": { }
}
